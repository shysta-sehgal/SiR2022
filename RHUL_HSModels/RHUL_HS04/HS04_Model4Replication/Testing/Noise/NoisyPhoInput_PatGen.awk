function pU(b){
    
    flip = 0;
	noise = 0.5;
	full = 0;

    x = rand();
    
    if(flip==1){
        if(full==0 && b == 1 && x < noise){
            printf 0;
        }else if(full==1 && x < noise){
            if(b==0){
                printf 1;
            }else{
                printf 0;
            }
        }else{
            printf b;
        }
    }else{
        if(b==1){
            printf "%0.2f",b-(noise*x);
        }else {
            if(full == 0){
                printf "0.00";
            }else {
                printf "%0.2f",0+(noise*x);
            }
        }
    }

    printf  " ";


}
